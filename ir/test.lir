
fn fact(n) {
    not_zero <- n != 0;
    jumpif not_zero not_zero;
zero:
    return 1, 0;
not_zero:
    prev <- n - 1;
    (k, prev) <- call fact(prev);
    val <- k * n;
    return val, 0;
}

fn main() {
    call native native_print_string(3, "fact :");
    (a, _) <- call fact(5);
    call native native_print_int(1, a);

    call native native_print_string(3, "pow :");
    (a, _) <- call native native_pow(1, 2, 1, 6);
    call native native_print_int(1, a);
}

