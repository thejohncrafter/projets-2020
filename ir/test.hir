
fn fact(n)
    vars: cond, n, m, val;
{
    cond <- n == (Int64, 1);
    
    if cond {
        val <- (Int64, 1);
    } else {
        m <- n - (Int64, 1);
        val <- call fact(m);
        val <- n * val;
    }

    return val;
}

fn main()
    vars: n, _;
{
    _ <- call native print_string("Hello world !");
    n <- call fact((Int64, 5));
    _ <- call native print_int(n);

    return (Int64, 0);
}

